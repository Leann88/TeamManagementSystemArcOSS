  <%= form_for(:instructor) do |f| %>
    <%= render 'shared/instructor_error_messages' %>
      <%= f.label :employeeid %>
      <%= f.text_field :employeeid %>
      <br />

      <%= f.label :password %>
      <%= f.password_field :password %>
      <br />

      <%= f.label :email %>
      <%= f.email_field :email %>
      <br />

      <%= f.label :firstname %>
      <%= f.text_field :firstname %>
      <br />

      <%= f.label :lastname %>
      <%= f.text_field :lastname %>
      <br />

      <%= f.label :program %>
      <%= f.text_field :program %>
      <br />

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>